{
  /*** SubMapping ***
  // See config_sub_mapping_gpu.json for the full description of the parameters.
  */
  "sub_mapping": {
    "so_name": "libsub_mapping.so",
    "enable_imu": true,
    "enable_optimization": true,
    // Keyframe update strategy
    "max_num_keyframes": 12,
    "keyframe_update_strategy": "OVERLAP",
    "keyframe_update_min_points": 1500,
    "keyframe_update_interval_rot": 1.57,
    "keyframe_update_interval_trans": 0.5,
    "max_keyframe_overlap": 0.9,
    // Relative pose factors
    "create_between_factors": true,
    "between_registration_type": "GICP",
    // Registration error factors
    "registration_error_factor_type": "VGICP",
    "keyframe_randomsampling_rate": 1.0,
    // "keyframe_voxel_resolution": 0.25,
    "keyframe_voxel_resolution": 0.2,
    "keyframe_voxelmap_levels": 1,
    "keyframe_voxelmap_scaling_factor": 2.0,
    // Post processing
    "submap_downsample_resolution": 0.4,
    // "submap_voxel_resolution": 0.5
    "submap_voxel_resolution": 0.3
  }
}